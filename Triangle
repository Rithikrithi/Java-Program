import java.util.Scanner;
class triangle{
    public static void main(String args[]){
        Scanner input = new Scanner(System.in);
        int a = input.nextInt();
        int b = input.nextInt();
        int c = input.nextInt();
        if(a+b>c && b+c>a && c+a>b){
            System.out.println("IT WILL FORM AN TRIANGLE");
        }else{
            System.out.println("IT WILL NOT FORM AN TRIANGLE");
        }
    }
}
